{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":dependencyDashboardApproval"],
  "baseBranchPatterns": ["work"],
  "automerge": true,
  "packageRules": [
    {
      "matchPackageNames": ["OpenTelemetry.Instrumentation.AWS"],
      "allowedVersions": "<0.12"
    },
    {
      "matchPackageNames": ["pdfjs-dist"],
      "allowedVersions": "<=5.3.93"
    }
  ]
}
