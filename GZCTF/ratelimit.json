{
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*:/api/puzzle/submit/*",
        "Period": "60s",
        "Limit": 3
      },
      {
        "Endpoint": "*:/api/account/register",
        "Period": "150s",
        "Limit": 10
      },
      {
        "Endpoint": "*:/api/*",
        "Period": "30s",
        "Limit": 120
      },
      {
        "Endpoint": "get:/*",
        "Period": "1s",
        "Limit": 20
      }
    ]
  }
}