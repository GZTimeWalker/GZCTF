{
  "compilerOptions": {
    "checkJs": true,
    "incremental": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "types": ["node"]
  },
  "include": ["plugins", "postcss.config.*", "prettier.config.*", "vite.config.*"]
}
